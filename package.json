{
  "name": "jira-tickets-tracker",
  "version": "1.2.1",
  "description": "Tracks jira tickets according to their status",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "codeCheck": "tsc && eslint . --ext .ts",
    "prepublish": "npm run build",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "author": "Hennadii Mishchevskyi, Vasyl Bulavynets",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gennadiii/jira-tickets-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/Gennadiii/jira-tickets-tracker/issues"
  },
  "homepage": "https://github.com/Gennadiii/jira-tickets-tracker/blob/master/README.md",
  "keywords": [
    "test",
    "jira"
  ],
  "files": [
    "dist/src/**/*",
    "dist/index.d.ts",
    "dist/index.ts"
  ],
  "devDependencies": {
    "@types/node": "20.9.2",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "8.54.0",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*test.ts$",
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "index.ts",
      "src/**/*ts"
    ]
  }
}
